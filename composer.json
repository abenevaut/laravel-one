{
    "name": "abenevaut/laravel-one",
    "description": "The Laravel One static web pages generator.",
    "keywords": ["laravel-one", "static", "web", "pages", "generator"],
    "homepage": "https://laravel-one.abenevaut.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/abenevaut/opensource/issues",
        "source": "https://github.com/abenevaut/opensource"
    },
    "authors": [
        {
            "name": "Antoine Benevaut",
            "email": "me@abenevaut.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "abenevaut/laravel-infrastructure": "^0.4",
        "illuminate/http": "^10.9",
        "illuminate/log": "^10.9",
        "illuminate/routing": "^10.9",
        "illuminate/translation": "^10.48",
        "illuminate/validation": "^10.48",
        "illuminate/view": "^10.9",
        "nunomaduro/termwind": "^1.15",
        "spatie/laravel-data": "^4.1",
        "spatie/laravel-markdown": "^2.4",
        "spatie/laravel-sitemap": "^7.0",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0",
        "mockery/mockery": "^1.5",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-faker": "^2.0",
        "phpstan/phpstan": "^2.0",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laravel-one"]
}
